{
    "name": "ASPA Dev Container",
    "dockerFile": "dockerfile/Dockerfile",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "GitHub.copilot-chat",
                "GitHub.copilot",
                "ms-vscode.vscode-typescript-next"
            ]
        }
    },
    "mounts": [
        "source=${localWorkspaceFolder},target=/repo,type=bind,consistency=cached"

    ]
}