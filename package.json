    "@next/font": "^14.2.4",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/express": "^4.17.21",
    "@types/swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "husky": "^9.1.4"
  }
}
